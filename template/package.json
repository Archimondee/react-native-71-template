{
  "name": "ProjectName",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "tsc && eslint . --ext .js,.jsx,.ts,.tsx --cache --fix",
    "start": "react-native start",
    "test": "jest",
    "clean": "cd android && ./gradlew clean && cd ..",
    "android:dev": "npx react-native run-android --mode=developmentdebug",
    "android:prod": "npx react-native run-android --mode=productiondebug",
    "android:staging": "npx react-native run-android --mode=stagingdebug",
    "ios:dev": "ENVFILE=.env.development && npx react-native run-ios --scheme 'Development' --simulator='iPhone 13 Pro'",
    "ios:staging": "ENVFILE=.env.development && npx react-native run-ios --scheme 'Staging' --simulator='iPhone 13 Pro'",
    "ios:prod": "ENVFILE=.env.production && npx react-native run-ios --scheme 'Production' --simulator='iPhone 13 Pro'",
    "build:apk:dev": "cd ./android && ./gradlew clean app:assembleDevelopmentRelease && cd ..",
    "build:apk:staging": "cd ./android && ./gradlew clean app:assembleStagingRelease && cd ..",
    "build:apk:prod": "cd ./android && ./gradlew clean app:assembleProductionRelease && cd ..",
    "build:aab:dev": "cd ./android && ./gradlew clean app:bundleProductionDebug && cd ..",
    "build:aab:prod": "cd ./android && ./gradlew clean app:bundleProductionRelease && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/core": "^6.4.9",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "react": "18.2.0",
    "react-native": "0.71.12",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-reanimated": "^3.3.0",
    "react-native-root-siblings": "^4.1.1",
    "react-native-root-toast": "^3.4.1",
    "react-native-safe-area-context": "^4.6.4",
    "react-native-screens": "^3.22.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-config-react-native-wcandillon": "^3.10.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.10",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
