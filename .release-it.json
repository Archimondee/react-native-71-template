{
  "git": {
    "publish": false,
    "commitMessage": "chore: release v${version}"
  },
  "npm": {
    "publish": false
  },
  "github": {
    "tokenRef": "TOKEN_RELEASE_IT",
    "release": true,
    "autoGenerate": true,
    "comments": {
      "submit": true
    }
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular",
      "infile": "CHANGELOG.md"
    }
  }
}
