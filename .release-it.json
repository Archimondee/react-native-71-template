{
  "git": {
    "changelog": "git log --pretty=format:\"* %s (%h)\" ${from}...${to}",
    "requireCleanWorkingDir": true,
    "requireBranch": false,
    "requireUpstream": true,
    "requireCommits": false,
    "requireCommitsFail": true,
    "commitsPath": "",
    "addUntrackedFiles": false,
    "commit": true,
    "commitMessage": "Release ${version}",
    "commitArgs": [],
    "tag": true,
    "tagExclude": null,
    "tagName": null,
    "tagMatch": null,
    "tagAnnotation": "Release ${version}",
    "tagArgs": [],
    "push": true,
    "pushArgs": ["--follow-tags"],
    "pushRepo": "https://github.com/archimondee/react-native-71-template.git"
  },
  "npm": {
    "publish": false
  },
  "github": {
    "tokenRef": "TOKEN_RELEASE_IT",
    "release": true,
    "autoGenerate": true,
    "comments": {
      "submit": true
    }
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular",
      "infile": "CHANGELOG.md"
    }
  }
}
